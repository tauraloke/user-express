{
    "name": "user-express",
    "version": "1.0.0",
    "description": "User management service built with Express and TypeScript",
    "main": "dist/server.js",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix"
    },
    "keywords": [
        "express",
        "typescript",
        "user-management",
        "sqlite"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "express": "^4.18.2",
        "sqlite3": "^5.1.6",
        "typeorm": "^0.3.17",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "express-validator": "^7.0.1",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "dotenv": "^16.3.1",
        "reflect-metadata": "^0.1.13",
        "class-validator": "^0.14.0",
        "class-transformer": "^0.5.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/bcryptjs": "^2.4.2",
        "@types/jsonwebtoken": "^9.0.0",
        "@types/cors": "^2.8.13",
        "@types/node": "^20.4.5",
        "@types/supertest": "^2.0.12",
        "@types/jest": "^29.5.3",
        "jest": "^29.5.0",
        "ts-jest": "^29.1.2",
        "supertest": "^6.3.3",
        "typescript": "^5.1.6",
        "ts-node-dev": "^2.0.0",
        "eslint": "^8.46.0",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0"
    }
}